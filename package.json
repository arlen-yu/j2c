{
  "name": "j2c",
  "version": "0.8.0-pre.4",
  "description": "A tiny CSS in JS solution.",
  "main": "dist/j2c.commonjs.js",
  "scripts": {
    "all": "npm run build && npm run lint && npm run nyan",
    "prepublish": "(in-publish && npm run -s all && git push --follow-tags) || not-in-publish",
    "build": "npm run -s clean && node scripts/build.js",
    "clean": "rm -f dist/*.js && rm -f dist/inline/*.js",
    "lint": " echo \"linting...\" && jshint dist/j2c.commonjs.js",
    "nyan": "mocha --ui qunit --reporter nyan",
    "test": "mocha --ui qunit",
    "cover": "mocha --ui qunit --require blanket --reporter mocha-lcov-reporter | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pygy/j2c.git"
  },
  "keywords": [
    "CSS",
    "css-in-js"
  ],
  "author": "Pierre-Yves GÃ©rardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pygy/j2c/issues"
  },
  "config": {
    "blanket": {
      "pattern": "//j2c\\.commonjs/"
    }
  },
  "homepage": "http://j2c.py.gy",
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "crass": "0.7.7",
    "expect.js": "0.3.1",
    "in-publish": "^2.0.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "surgicate": "0.0.3",
    "uglify-js": "^2.6.0"
  }
}
